
<div class="search_results">
  <ul><% @first_ten.each do |recipe| %>
   <li class="search_results_list_items"><a href="http://yummly.com/recipe/<%=recipe["id"]%>" ><%= recipe["recipeName"] %></a></br>
       <% recipe["ingredients"].each do |ingredient| %>
          <%= ingredient %><br/>
        <% end %>
   </li>
  <% end %></ul>
</div>

